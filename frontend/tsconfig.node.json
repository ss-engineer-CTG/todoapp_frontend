{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "noEmit": true,
    "types": ["node"]
  },
  "include": ["vite.config.js", "postcss.config.js"],
  "exclude": ["node_modules", "dist"]
}